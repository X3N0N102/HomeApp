<Window x:Class="HomeApp.HomePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HomeApp"
        mc:Ignorable="d"
        Title="HomePage" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid Background="#FF212121">
        <Image x:Name="cabinet" HorizontalAlignment="Left" Height="116" Margin="200,70,0,0" VerticalAlignment="Top" Width="100" Source="Img/cabinet.png" MouseLeftButtonUp="cabinetEnter"/>
        <Image Height="116" Margin="491,244,200.6,0" VerticalAlignment="Top" Source="Img/Refrigerator.png"/>
        <Image Height="116" Margin="200,244,491.6,0" VerticalAlignment="Top" Source="Img/pantry.png" MouseLeftButtonUp="fridgeEnter"/>
        <Image Height="116" Margin="493,70,200.6,0" VerticalAlignment="Top" Source="Img/frezeer.png"/>
    </Grid>
</Window>
